{
  "name": "hypernet.id",
  "version": "0.0.1",
  "description": "Hypernet.ID ",
  "private": true,
  "engines": {
    "node": ">= 10.x.x"
  },
  "author": "Charlie Sibbach",
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "alias": "npx lerna run alias",
    "bootstrap": "docker-compose up --detach shared-sql && sleep 60 && npx lerna run bootstrap",
    "build": "yarn clean && yarn compile && yarn postCompile",
    "clean": "rimraf ./packages/*/dist ./packages/*/tsconfig.tsbuildinfo",
    "clean-docker": "docker-compose down",
    "clean-node": "rimraf node_modules packages/*/node_modules yarn.lock",
    "compile": "tsc --build && yarn alias",
    "dev": "docker-compose up -d onboarding && cd packages/combined && yarn start",
    "dev-onboarding-only": "cd packages/onboarding && yarn start",
    "dev-server-only": "cd packages/combined && yarn start",
    "docker-push": "npx lerna run docker-push",
    "dockerize": "cd packages/combined && yarn dockerize",
    "format": "prettier . --write && yarn format:pkg",
    "format:pkg": "format-package -w",
    "generate-docs": "npx lerna run generate-docs",
    "lint": "eslint \"packages/*/{src,test}/**/*.{ts,tsx}\" --quiet",
    "lint:fix": "eslint \"packages/*/{src,test}/**/*.{ts,tsx}\" --quiet --fix",
    "postCompile": "npx lerna run postCompile",
    "prisma-generate": "npx lerna run prisma-generate",
    "prisma-migrate-deploy": "npx lerna run prisma-migrate-deploy",
    "prisma-migrate-dev": "npx lerna run prisma-migrate-dev",
    "prisma-deploy": "npx lerna run prisma-deploy",
    "prisma-push": "npx lerna run prisma-push",
    "prisma-seed": "npx lerna run prisma-seed",
    "prod-tunnel": "npx run-script-os",
    "start": "docker-compose up combined",
    "test": "npx lerna run test"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.14.0",
    "**/@typescript-eslint/parser": "^4.22.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mysql": "^2.15.19",
    "@types/object-hash": "^2.2.1",
    "@types/redis": "^2.8.32",
    "@types/redlock": "^4.0.2",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.22.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "chai": "^4.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^6.5.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^5.0.1",
    "cucumber-tsflow": "^4.0.0-rc.1",
    "env-cmd": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "file-loader": "^6.2.0",
    "format-package": "^6.1.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.3.2",
    "http-server": "^0.12.3",
    "https-browserify": "^1.0.0",
    "image-webpack-loader": "^7.0.1",
    "jest": "^26.4.2",
    "jest-cli": "^25.1.0",
    "jest-when": "^3.0.1",
    "lerna": "^4.0.0",
    "lint-staged": ">=10",
    "metro-react-native-babel-preset": "^0.59.0",
    "moleculer-repl": "^0.6.2",
    "monaco-editor-webpack-plugin": "2.0.0",
    "mysql2": "^2.3.3",
    "node-sass": "6.0.0",
    "nodemon": "^2.0.15",
    "os-browserify": "^0.3.0",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.2.0",
    "prettier": "^2.2.1",
    "prisma": "^3.2.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "sass-loader": "8.0.0",
    "source-map-loader": "^2.0.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.1.1",
    "style-loader": "^2.0.0",
    "testdouble": "^3.16.2",
    "testdouble-jest": "^2.0.0",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "tsc-alias": "^1.3.9",
    "tsc-watch": "^4.5.0",
    "tsconfig-paths": "^3.11.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typedoc": "^0.20.16",
    "typescript": "^4.7.4",
    "url-loader": "3.0.0",
    "wait-on": "^5.2.0",
    "webpack": "^5.28.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "tsc": "^2.0.4"
  }
}
