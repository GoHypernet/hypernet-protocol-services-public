{
	"compilerOptions": {
		"skipLibCheck": true,
		"declaration": true,
		"downlevelIteration": true,
		"target": "ES2019",
		"baseUrl": ".",
		"module": "commonjs",
		"removeComments": false,
		"moduleResolution": "node",
		"preserveConstEnums": true,
		"sourceMap": true,
		"strict": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"declarationMap": true,
		"forceConsistentCasingInFileNames": true,
		"allowSyntheticDefaultImports": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noImplicitAny": false,
		"resolveJsonModule": true,
		"jsx": "react",
		"composite": true,
		"paths": {
			"@authenticationContracts/*": ["./*"],
			"@authentication/*": ["./*"],
			"@authorizationContracts/*": ["./*"],
			"@authorization/*": ["./*"],
			"@avalanche/*": ["./*"],
			"@chainContracts/*": ["./*"],
			"@ethereumChainBase/*": ["./*"],
			"@events/*": ["./*"],
			"@fuji/*": ["./*"],
			"@ipfsContracts/*": ["./*"],
			"@ipfs/*": ["./*"],
			"@mumbai/*": ["./*"],
			"@objects/*": ["./*"],
			"@onboarding/*": ["./*"],
			"@polygon/*": ["./*"],
			"@queues/*": ["./*"],
			"@rinkeby/*": ["./*"],
			"@security/*": ["./*"],
			"@utils/*": ["./*"],
			"@sharedComponents/*": ["./*"],
			"@tests/*": ["../test/*"],
			"@mock/*": ["../test/mock/*"]
		}
	}
}
